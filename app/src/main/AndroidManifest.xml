<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.my_contacts">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.My_Contacts">
        <activity android:name=".UI.ContactDetailsActivity"></activity>
        <activity android:name=".UI.AddContactActivity" />
        <activity android:name=".ContactDetaisActivity" />
        <activity android:name=".UI.EditContactActivity" />
        <activity android:name=".Add_Contact" />
        <activity android:name=".Display_Activity1" />
        <activity
            android:name=".UI.MainActivity"
            android:noHistory="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>